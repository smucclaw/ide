{
  "name": "l4-extension",
  "displayName": "L4 extension",
  "description": "Language support for L4",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:l4-extension.helloWorld",
    "onCommand:ide-prototype.mkMarkdown",
    "onCommand:ide-prototype.mkGraph"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "l4-extension.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "ide-prototype.mkMarkdown",
        "title": "Generate Markdown"
      },
      {
        "command": "ide-prototype.mkGraph",
        "title": "Generate Graph"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "lint": "gts lint",
    "test": "node ./out/test/runTest.js",
    "clean": "gts clean",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "posttest": "npm run lint"
  },
  "devDependencies": {
    "@types/vscode": "^1.52.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.11.2",
    "eslint": "^7.15.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.0.3",
    "vscode-test": "^1.4.1",
    "gts": "^3.0.3"
  },
  "dependencies": {}
}
